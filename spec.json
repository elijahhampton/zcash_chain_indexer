            "properties": {
              "id": {"type": "string"},
              "monitored": {"type": "boolean"},
              "chainValue": {"type": "number", "optional": true},
              "chainValueZat": {"type": "number", "optional": true},
              "valueDelta": {"type": "number", "optional": true},
              "valueDeltaZat": {"type": "number", "optional": true}
